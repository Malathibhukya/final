class Solution {
    public int removeDuplicates(int[] nums) {
        int c=1;
        ArrayList<Integer> a=new ArrayList<>();
        a.add(nums[0]);
        int n=nums.length;
        for(int i=0;i<n-1;i++)
        {
            if(nums[i]!=nums[i+1])
            {
            c++;
            a.add(nums[i+1]);
            }
        }
        for(int i=0;i<a.size();i++)
        {
            nums[i]=a.get(i);
        }
        return c;
        
    }
}
